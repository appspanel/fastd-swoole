{
    "name": "fastd/swoole",
    "description": "FastD swoole.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "fastd/packet": "^1.4",
        "fastd/http": "^3.1",
        "symfony/console": "6.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "FastD\\Swoole\\": "src"
        },
        "files": [
            "helpers.php"
        ]
    },
    "suggests": {
        "ext-swoole": "Allow php swoole extension.",
        "ext-inotify": "Allow php inotify extension."
    },
    "repositories": [
        {
            "name": "fastd/http",
            "type": "vcs",
            "url": "https://github.com/appspanel/fastd-http.git"
        }
    ]
}
